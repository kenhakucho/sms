<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="wrapper">
      <form action="/" method="post" class="board-form">
        ルーム名: <input type="text" name="title" class="input" required><br>
        <br>
        <button type="submit" class="submit">ルーム作成</button>
        <br>
      </form>
      <ul class="main-list">
        <% roomList.forEach(function(room) { %>
          <li class="main-list__item">
            <div class="room">
              <p class="room_icon"><%= room.r_icon %></p>
              <p class="room_name"><%= room.r_name %></p>
            </div>
          </li>
        <% }); %>
      </ul>
    </div>
  </body>
</html>
